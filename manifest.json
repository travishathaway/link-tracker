{
    "manifest_version": 2,

    "name": "Click Tracker",
    "description": "This plugin keeps track of all links you've clicked.",
    "version": "1.0",

    "permissions" : [
        "http://localhost:8111/"
    ],

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                    "/js/external/jquery.js", 
                    "/js/clickTracker.js"
                ]
        }
    ]
}
